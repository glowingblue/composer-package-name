{
  "name": "composer-package-name",
  "repository": {
    "type": "git",
    "url": "https://github.com/glowingblue/composer-package-name.git"
  },
  "displayName": "GB Composer package name",
  "description": "Command to get the package name of the currently open project by parsing composer.json",
  "version": "1.0.0",
  "license": "MIT",
  "author": "dev@glowingblue.com",
  "publisher": "glowingblue",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:glowingblue-composer-package-name.getName"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "glowingblue-composer-package-name.getName",
        "title": "GB Composer: Get Package Name"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@types/glob": "^7.1.4",
    "@types/node": "14.x",
    "glob": "^7.1.7",
    "typescript": "^4.4.4"
  }
}